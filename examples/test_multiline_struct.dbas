' Test multi-line struct literals

TYPE Button
    DIM text AS STRING
    DIM width AS INTEGER
    DIM height AS INTEGER
END TYPE

SUB Main()
    ' Multi-line struct literal
    DIM btn AS Button = Button{
        text: "OK",
        width: 80,
        height: 25
    }

    PRINT "Button:"; btn.text
    PRINT "Size:"; btn.width; "x"; btn.height
END SUB
