{
  "name": "dbasic",
  "displayName": "DBasic",
  "description": "DBasic language support - BASIC-to-Go transpiler",
  "version": "0.2.0",
  "publisher": "zditech",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zditech/dbasic"
  },
  "contributes": {
    "languages": [
      {
        "id": "dbasic",
        "aliases": [
          "DBasic",
          "dbasic",
          "DBASIC"
        ],
        "extensions": [
          ".dbas",
          ".dbasic"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/dbasic.png",
          "dark": "./icons/dbasic.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "dbasic",
        "scopeName": "source.dbasic",
        "path": "./syntaxes/dbasic.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "dbasic",
        "path": "./snippets/dbasic.json"
      }
    ]
  }
}
